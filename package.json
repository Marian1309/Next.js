  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "auth:gen": "npx @better-auth/cli@latest generate",
    "build": "next build --turbopack",
    "build:analyze": "npm run analyze && npm run start",
    "build:production": "NODE_ENV=production next build --turbopack",
    "check:up": "ncu -i",
    "clean": "rimraf bun.lock && rimraf .next && rm -rf node_modules",
    "convex": "convex dev",
    "db:gen": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:pull": "drizzle-kit pull",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "deploy": "vercel --prod",
    "dev": "next dev --turbo",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "format": "prettier . -w",
    "knip": "knip",
    "lint": "oxlint --fix",
    "lint-staged": "lint-staged",
    "optimize": "npm run build && npm run analyze",
    "prepare": "husky",
    "prod": "next build && next start",
    "shadcn": "shadcn add",
    "shadcn:all": "shadcn add --all",
    "start": "next start",
    "test:unit": "bunx vitest run --passWithNoTests",
    "test:e2e": "bunx playwright test",
    "unused": "knip --include dependencies,unlisted"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bun run format",
      "bun run lint"
    ],
    "*": "pretty-quick --staged"
  },
  "browserslist": {
    "production": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 safari versions",
      "last 2 edge versions"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
